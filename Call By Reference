#include <stdio.h>
#include <stdlib.h>
#include <iostream>
using namespace std;

void swap(int &a,int &b){
    int temp;
    temp=a;
    a=b;
    b=temp;
    }

int main()
{
    int x,y;
    x=5;
    y=10;
    swap(x,y);
    printf("swaped numbers are\n %d\n %d",x,y);
    
}
/*To make an function as call by reference we just need to add & in the 
parameters, these are the references*/
